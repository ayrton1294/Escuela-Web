@*@model List<ObjetoEscuelaBase>*@


<table class="table table-dark">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
        <tr>
            <td>@obj.Nombre</td>
            <td>@obj.Id</td>
        </tr>            
        }
    </tbody>
</table>